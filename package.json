{
  "name": "clever-env",
  "description": "Strongly typed environment variables with validation and sanitization.",
  "license": "MIT",
  "version": "0.0.0",
  "homepage": "https://github.com/screfy/clever-env#readme",
  "author": "screfy <me@screfy.com> (https://screfy.com)",
  "repository": "github:screfy/clever-env",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist && tsup src/index.ts --format cjs,esm --dts",
    "test": "jest",
    "prepare": "husky install",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "bugs": {
    "url": "https://github.com/screfy/clever-env/issues"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "dotenv",
    "environment",
    "typescript",
    "validation",
    "environment-variables",
    "typesafe",
    "env"
  ],
  "engines": {
    "node": ">=12"
  },
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.18",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "tsup": "^5.11.13",
    "typescript": "^4.5.5"
  }
}
